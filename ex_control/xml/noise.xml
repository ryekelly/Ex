<stimulus name="noise" repeats="1" ex="ex_activeFixation" bgColor="0 0 0">
    <conditions type="movie" runline="movieprefix,suffix numframes dwell startframe centerx centery" movieprefix="example/noise">
        <suffix>
            <value>1:10</value>
        </suffix>
        <dwell>
            <value>4</value>
        </dwell>
        <startframe>
            <value>1</value>
        </startframe>
        <numframes>
            <value>100</value>
        </numframes>
        <centerx>
            <value>34</value>
        </centerx>
        <centery>
            <value>61</value>
        </centery>
    </conditions>
    <params>
        <fixX>0</fixX>
        <fixY>0</fixY>
        <fixRad>5</fixRad>
        <fixColor>[0 0 255]</fixColor>
        <preStimFix>150</preStimFix>
        <timeToFix>1000</timeToFix>
        <saccadeTime>600</saccadeTime>
        <stayOnTarget>150</stayOnTarget>
        <saccadeLength>150</saccadeLength>
        <noFixTimeout>1000</noFixTimeout>
    </params>
    <randoms>
        <saccadeDir>0:359</saccadeDir>
    </randoms>
</stimulus>